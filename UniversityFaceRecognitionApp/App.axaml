<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="UniversityFaceRecognitionApp.App"
             xmlns:local="using:UniversityFaceRecognitionApp"
             RequestedThemeVariant="Light">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="avares://UniversityFaceRecognitionApp/Icons.axaml"></StyleInclude>
    </Application.Styles>
</Application>